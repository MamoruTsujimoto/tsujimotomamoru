    "date-fns": "^2.27.0",
    "gray-matter": "^4.0.3",
    "modern-css-reset": "^1.4.0",
    "next": "12.0.4",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.26.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark": "^14.0.2",
